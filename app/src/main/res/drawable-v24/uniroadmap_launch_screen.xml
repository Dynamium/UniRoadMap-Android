<!--Launch Screen: To use this drawable for your own launch screen, replace the bitmap src attribute values with your own logo(s)-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:opacity="opaque">
    <item android:bottom="@dimen/uniroadmap_logo_bottom_spacing">
        <bitmap
            android:gravity="center"
            android:src="@drawable/uniroadmap_launch_screen"
            tools:ignore="ResourceCycle" />
    </item>
    <item android:bottom="@dimen/launchscreen_dynamium_logo_bottom">
        <bitmap
            android:gravity="bottom|center_horizontal"
            android:src="@drawable/ic_dynamium_long_logo" />
    </item>
</layer-list>